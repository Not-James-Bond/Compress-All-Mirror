{
    "manifest_version": 2,
    "name": "Compress All",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png"
      },
      "permissions": [
        "contextMenus"
      ],
      "icons": {
        "16": "icon-bitty.png",
        "48": "icon-small.png",
        "128": "icon-large.png"
      },

    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ]  ,
      "background": {
        "scripts": ["background.js"],
        "persistent": true
      } 
}

